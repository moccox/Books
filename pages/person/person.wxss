/* pages/person/person.wxss */

/*上半部分背景*/
.bg_top{
  width: 100%;
  height: 30%;
  display: flex;
  background-image: url('data:image/png;base64,');
  flex-direction: column;
  align-items: center;
}

/*下半部分背景*/
.bg_bottom{
  width: 100%;
  height: 70%;
  background-color: rgb(224,224,224);
  display: flex;
  flex-direction: column;
}

/*获取登录授权按钮*/
.loginbt{
  width: 180rpx;
  height: 80rpx;
  background: #fff;
  display: flex;
  margin-top: 100rpx;
}

/*放置用户名区域*/
.name{
  width: 300rpx;
  height: 80rpx;
  background-color: transparent;
  color: #fff;
  display: flex;
  margin-top: 100rpx;
}

/*放置个人信息子列表区域*/
.personalView{
  width: 98%;
  height: 248rpx;
  background-color: #ccc;
  display: flex;
  flex-direction: column;
  margin: 0 1% 0 1%;
  transition: height 0.5s;
}
/*放置个人信息子列表区域-隐藏*/
.personalView_hidden{
  width: 100%;
  height: 0;
  background-color: #ccc;
  display: flex;
  flex-direction: column;
  margin: 0 2% 0 2%;
  transition: height 200ms;
}

/*tap*/
.myTap{
  width: 100%;
  height: 80rpx;
  background-color: #fff;
  display: flex;
  flex-direction: row;
}

/*标签（大）*/
.myLab_one{
  color: #1995f4;
  margin:2% 10rpx 2% 30rpx;
}

/*标签（小）*/
.myLab_two{
  color: #aaa;
  font-size: 40rpx;
  margin:2% 10rpx 2% 30rpx;
}